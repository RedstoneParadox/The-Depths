{
  "schemaVersion": 1,
  "id": "thedepths",
  "version": "${version}",

  "name": "Example Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "LGPL v3.0",
  "icon": "assets/thedepths/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.redstoneparadox.thedepths.TheDepths"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "io.github.redstoneparadox.thedepths.client.TheDepthsClient"
      }
    ]
  },
  "mixins": [
    "thedepths.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": ">=0.3.2",
    "fabric-language-kotlin": ">=1.3.40"
  },
  "suggests": {
    "flamingo": "*"
  }
}
